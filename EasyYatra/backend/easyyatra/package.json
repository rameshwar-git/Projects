{
    "name": "easyyatra",
    "version": "1.0.0",
    "description": "easyyatra is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpileOnly ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
        "prod": "tsc && node ./build/src/server.js"
    },
    "keywords": [
        "express",
        "backend",
        "easyyatra"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "@types/express": "^4.17.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.23.3",
        "http-status": "^1.3.2",
        "mandatoryenv": "^1.1.6",
        "module-alias": "^2.2.2",
        "mongodb": "^3.3.3",
        "mongoose": "^8.16.3",
        "morgan": "^1.10.0",
        "rotating-file-stream": "^1.4.6",
        "ts-node-dev": "^1.0.0-pre.43",
        "typescript": "^3.6.3"
    },
    "devDependencies": {
        "@types/node": "^18.19.119"
    }
}
